{
    "name": "baek/sw-playground",
    "description": "SW Playground",
    "type": "shopware-platform-plugin",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SwPlayground\\": "src/"
        }
    },
    "require": {
        "shopware/core": "~v6.6",
        "symfony/expression-language": "^v7.1.6.0",
        "openai-php/client": "^0.10.3"
    },
    "extra": {
        "shopware-plugin-class": "SwPlayground\\SwPlayground",
        "label": {
            "de-DE": "SwPlayground",
            "en-GB": "SwPlayground"
        },
        "description": {
            "de-DE": "Collection of dev/convenience tools for Shopware 6",
            "en-GB": "Collection of dev/convenience tools for Shopware 6"
        },
        "manufacturerLink": {
            "de-DE": "https://github.com/bschulzebaek/sw-playground",
            "en-GB": "https://github.com/bschulzebaek/sw-playground"
        },
        "supportLink": {
            "de-DE": "https://github.com/bschulzebaek/sw-playground",
            "en-GB": "https://github.com/bschulzebaek/sw-playground"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true,
            "php-http/discovery": true
        }
    }
}
